	@Value("${}mq.demo.routingkey")
	String routing_key;
	
	@RabbitListener(queues = queue_name)
	public void consumeMessageFromQueue(OrderStatus orderStatus) {
		System.out.println("Message received from Queue : "+orderStatus);
