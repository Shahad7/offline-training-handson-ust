spring:
 application:
  name: hello-gateway
  
 cloud:
  gateway:
   routes:
    - id: helloservice-route
      uri: http://localhost:8051
      predicates :
       - Path=rest/service/**
    - id: anotherservice-route
      uri: http://localhost:8052
      predicates:
       - Path=rest/service2/**
server:
 port: 8070